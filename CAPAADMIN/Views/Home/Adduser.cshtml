
@{
    ViewBag.Title = "Adduser";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<form  class="m-5 h-100 border border-3 border-light rounded p-3">
    <div class="row">
        <div class="col">
            <div class="mb-3 ">
                <label for="exampleInputEmail1" class="form-label">Nombre Completo</label>
                <input type="text" class="form-control" id="nombrecompleto" aria-describedby="Descrip">
            </div>

            <div class="mb-3">
                <label for="exampleInputPassword1" class="form-label">Edad</label>
                <input type="number" class="form-control" id="edad">
            </div>

            <div class="mb-3">
                <label for="exampleInputPassword1" class="form-label">Numero</label>
                <input type="number" class="form-control" id="numero">
            </div>
            <div class="mb-3">
                <label for="exampleInputPassword1" class="form-label">Contrasena</label>
                <input type="password" class="form-control" id="edad">
            </div>
        </div>
        <div class="col">
            <div class="mb-3">
                <label for="exampleInputPassword1" class="form-label">Usuario</label>
                <input type="text" class="form-control" id="usuario">
            </div>
            <div class="mb-3">
                <label for="exampleInputPassword1" class="form-label">Email</label>
                <input type="Text" class="form-control" id="email">
            </div>
            <div class="row mb-3">
                <div class="row mb-3">
                    <label for="exampleInputPassword1" class="form-label me-2 ">FBautismo</label>
                    <input type="date" class="form-control" id="Fbautismo">
                </div>

                <div class="row mb-3">
                    <label for="exampleInputPassword1" class="form-label me-2 ms-3">FIngreso</label>
                    <input type="date" class="form-control w-100" id="exampleInputUbicacion">
                </div>            
            </div>
        </div>
    </div>
  
  

      
    <div class="d-flex mb-5">
        <label for="exampleInputPassword1" class="form-label me-2">Sexo</label>
        <select class="form-select" aria-label="Default select example">
            <option selected value="1">M</option>
            <option value="2">F</option>
        </select>
        <label for="exampleInputPassword1" class="form-label me-2">Cargo</label>
        <select class="form-select" aria-label="Default select example">
            <option selected value="1">Miembro</option>
            <option value="2">Lider</option>
            <option value="3">Diacono</option>
        </select>
    </div>
    <button type="submit" class="btn btn-primary">Aceptar</button>
</form>
