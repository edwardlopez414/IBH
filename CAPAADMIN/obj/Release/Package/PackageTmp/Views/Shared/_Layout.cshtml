    <script type="text/javascript">

        function ocultar() {
            var x = document.getElementById("dvrih");
            if (x.style.display === "none") {
                x.style.display = "";

            } else {
                x.style.display = "none";

            }
        }
        function desplazar() {
            var sr = document.getElementById("dvrih");
            var dr1 = document.getElementById("dvcol");
            var dasboard = document.getElementById('render');
            if (sr.style.height === "100vh") {
                sr.style.height = "5vh";
                dr1.style.display = "none";
                dasboard.style.display = 'block';
            } else {
                sr.style.height = "100vh";
                dr1.style.display = "block";
                dasboard.style.display = 'none';
            }
        }
       

        function search() {
            var inputValue = document.getElementById('buscar');
            var url = "@Url.Action("MIEMBROS", "Home")";
            var data = { cedula: inputValue.value};
            $.post({
                url, data, beforeSend: function () {}
            })
                .fail(function () {
                    alert("error");
                })
                .done(function (data) {
                        var Nombre = document.getElementById('nombrecompleto');
                        var cedula = document.getElementById('cedula');
                        var edad = document.getElementById('edad');
                        var numero = document.getElementById('numero');
                        var usuario = document.getElementById('usuario');
                        var email = document.getElementById('email');
                        var direccion = document.getElementById("direccion");
                        var Iduser = document.getElementById("IduserBD");
                        console.log(data.data);
                        Nombre.value = data.data[0].Nombre_Completo;
                        cedula.value = data.data[0].Cedula;
                        edad.value = data.data[0].Edad;
                        direccion.value = data.data[0].Direccion;
                        numero.value = parseInt(data.data[0].Nro_contacto);
                        usuario.value = data.data[0].usuario;
                       email.value = data.data[0].Email;
                    Iduser.value = data.data[0].Id_Usuario;
                    
                });
        }

    </script>

