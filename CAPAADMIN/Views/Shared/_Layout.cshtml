        </div>

    </main>


   
    <script type="text/javascript">
        function ocultar() {
            var x = document.getElementById("dvrih");
            if (x.style.display === "none") {
                x.style.display = "";
                document.getElementById('dvnav2').style.width = '100%';
            } else {
                x.style.display = "none";
                document.getElementById('dvnav2').style.width = '70vh%';
            }
        }
    </script>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
